<ul class="breadcrumb" style="margin: 5px;">
<% breadcrumb.each do |bc| %>
  <% if bc[:id].nil? %>
  <li><a id="gbtn-<%= SecureRandom.uuid %>" href="/mdwiki">HOME</a></li>
  <% else %>
  <li><a id="gbtn-<%= SecureRandom.uuid %>" href="/mdwiki/<%= bc[:id] %>"><%= bc[:title] %></a></li>
  <% end %>
<% end %>
<li class="active"><%= title %></li>
</ul>
